<package type="addon" name="nuxeo-custom-thumbnails" version="@VERSION@">
  <title>Nuxeo Custom Thumbnails</title>
  <description>
    <p>This addon activates the Nuxeo Custom Thumbnails addon.</p>
    <p>This addon adds new features as explained <a href="https://github.com/nuxeo-sandbox/nuxeo-custom-thumbnails">here</a></p>
  </description>
  <home-page>https://github.com/nuxeo-sandbox/nuxeo-custom-thumbnails</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <hotreload-support>false</hotreload-support>
  <require-terms-and-conditions-acceptance>false</require-terms-and-conditions-acceptance>
  <nuxeo-validation>waiting_nuxeo_certified</nuxeo-validation>
  <production-state>development</production-state>
  <supported>false</supported>
  <platforms>
    <platform>cap-@VERSION@</platform>
    <platform>cap-7.2</platform>
    <platform>cap-7.3</platform>
  </platforms>
<!-- 
  <dependencies>
    <package>nuxeo-adullact-service:@VERSION@</package>
  </dependencies>
 -->
  <license>LGPL</license>
  <license-url>http://www.gnu.org/licenses/lgpl.html</license-url>
</package>
